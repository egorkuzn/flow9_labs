// вычисляющую n первых чисел фиббоначчи: 0, 1, 1, 2, 3, 5, ....
// Сделать ее
// а) рекурсивной
// б) с хвостовой рекурсией
// в) с использованием ссылок на массив, сложности O( n ).
// Использовать: fold, concat, refArrayPush
import runtime;
import string;

fib(n : int) -> [int];

fib(n){
    if(n == 0){
        [0];
    } else if(n == 1){
        [0, 1];
    } else{
        concat(fib(n - 1), [fib(n - 1)[n - 1] + fib(n - 2)[n - 2]]);
    }
}



main(){
    println(fib(6));
}
